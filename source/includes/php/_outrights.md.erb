```php
<?php
require 'authentication';

// Outrights
// Endpoint: /api/v2/sports/outright_list/<sportId>

$outrights = new \Sportsbook\OutrightList();
$sportId = 1;

try {
	$outrights->parseFromString(getApiResponse('outright_list/' . $sportId));
} catch (Exception $e) {
	die('Unable to get outrights list: ' . $e->getMessage() . PHP_EOL);
}

$outrightIds = array_values(array_map(function (\Sportsbook\Outright $outright) {
	return $outright->getId();
}, $outrights->getOutrights()));

echo 'Outrights: ' . count($outrightIds) . ' outrights for sport #' . $sportId . PHP_EOL;
